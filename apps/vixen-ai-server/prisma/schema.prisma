datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Role {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Notification {
  id        String                     @id @default(cuid())
  createdAt DateTime                   @default(now())
  updatedAt DateTime                   @updatedAt
  message   String?
  read      Boolean?
  typeField EnumNotificationTypeField?
  user      String?
}

model Integration {
  id            String                    @id @default(cuid())
  createdAt     DateTime                  @default(now())
  updatedAt     DateTime                  @updatedAt
  configuration Json?
  connected     Boolean?
  name          String?
  typeField     EnumIntegrationTypeField?
}

enum EnumNotificationTypeField {
  Option1
}

enum EnumIntegrationTypeField {
  Option1
}
